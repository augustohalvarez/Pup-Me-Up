{
  "manifest_version": 2,
  "name": "Pup Me Up",
  "version": "1.0",
  "description": "Help you get through your day.",
  // "icons": {
  //     "16": "small.png",
  //     "48": "large.png"
  // },
  "browser_action": {
      "default_title": "Pup Me Up",
      "default_popup": "index.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
  {
    "matches": ["http://*/*"],
    "js": ["jquery-3.2.1.min.js", "doggie.js"]
  }
]
}